<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Validation</servlet-name>
  	<display-name>Validation</display-name>
  	<description></description>
  	<servlet-class>simplilearn.Validation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AcademyInfo</servlet-name>
  	<display-name>AcademyInfo</display-name>
  	<description></description>
  	<servlet-class>simplilearn.AcademyInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AlltheClasses</servlet-name>
  	<display-name>AlltheClasses</display-name>
  	<description></description>
  	<servlet-class>simplilearn.AlltheClasses</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClassandSubjects</servlet-name>
  	<display-name>ClassandSubjects</display-name>
  	<description></description>
  	<servlet-class>simplilearn.ClassandSubjects</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Reports</servlet-name>
  	<display-name>Reports</display-name>
  	<description></description>
  	<servlet-class>simplilearn.Reports</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Validation</servlet-name>
  	<url-pattern>/Validation/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AcademyInfo</servlet-name>
  	<url-pattern>/AcademyInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AlltheClasses</servlet-name>
  	<url-pattern>/AlltheClasses</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClassandSubjects</servlet-name>
  	<url-pattern>/ClassandSubjects</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Reports</servlet-name>
  	<url-pattern>/Reports</url-pattern>
  </servlet-mapping>
  

</web-app>
